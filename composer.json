  },
  "minimum-stability": "dev",
  "prefer-stable" : true,
  "scripts": {
      "post-install-cmd": [
        "cp -R ./core_patches/* ./docroot",
        "cp dev/robots.txt ./docroot/",
        "cp dev/google6cc4277693acab0c.html ./docroot/",
        "cp dev/088a370aa369dd97af14452c3d3527f8.txt ./docroot/",
        "cp dev/robots-central.txt ./docroot/",
        "cp dev/.htaccess ./docroot/",
        "cp ./environments/env.settings.php ./docroot/sites/default/",
        "cp -R dev/.well-known ./docroot",
        "bash environments/mailchimp_lib.sh"
      ]
  },
  "config": {
    "vendor-dir": "vendor"
  },
  "extra": {
    "scripts-dev": {
      "post-install-cmd": [
        "bash dev/setup.sh"
      ],
      "platform": {
        "php": "7.3"
      }
    },
    "installer-paths": {
      "docroot": ["type:drupal-core"],
      "docroot/sites/all/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
      "docroot/sites/all/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "preserve-paths": [
      "docroot/sites/all/modules/contrib",
      "docroot/sites/all/modules/custom",
      "docroot/sites/all/modules/patches",
      "docroot/sites/all/modules/features",
      "docroot/sites/all/themes/contrib",
      "docroot/sites/all/themes/custom",
      "docroot/sites/all/libraries",
      "docroot/sites/all/drush",
      "docroot/sites/default/settings.php",
      "docroot/sites/default/files",
      "docroot/sites/default/private"
    ],
    "patches": {
      "drupal/entity_translation": {
        "Better locale integration": "docroot/sites/all/modules/patches/entity_translations_v2.patch",
        "Disable translation unavailable message": "https://www.drupal.org/files/issues/disable-translation-not-available-message.patch"
      },
      "drupal/file_admin": {
        "Separate permission `publish files`": "docroot/sites/all/modules/patches/file_admin.publish-files-permission.patch"
      },
      "drupal/commerce": {
        "Added additional check for commerce_product title callback" : "docroot/sites/all/modules/patches/title_callback.product.commerce.patch"
      },
      "drupal/domain_locale": {
        "Hide disabled languages" : "docroot/sites/all/modules/patches/domain_locale.disabled-languages.patch",
        "Prevent sending langcode as object into query" : "docroot/sites/all/modules/patches/domain_locale.langcode-object.patch"
      },
      "drupal/memcache": {
        "Fix order": "docroot/sites/all/modules/patches/memcache-fix_order_pecl_memcache-2415109-7.patch"
      },
      "drupal/addressfield": {
        "display fields if no country, country value -none-, custom trigger" : "docroot/sites/all/modules/patches/addressfield_v5.patch",
        "UK Counties list": "https://www.drupal.org/files/issues/addressfield-uk_counties-1630834-4.patch",
        "CN administrative area numeric to iso" : "docroot/sites/all/modules/patches/addressfield-cn-administrative-area-iso.patch"
      },
      "drupal/commerce_features": {
        "Exportable Commerce Order types": "docroot/sites/all/modules/patches/commerce_features.remove_support_of_commerce_order_types.patch"
      },
      "drupal/password_policy": {
        "Check password with origin password": "docroot/sites/all/modules/patches/password_policy.patch"
      },
      "drupal/feeds_tamper": {
        "Empty multi values fields patch for explode plugin of feeds_tamper": "docroot/sites/all/modules/patches/feeds_tamper_explode_multivalues_empty.patch"
      },
      "drupal/securepages": {
        "No active batch fix": "https://www.drupal.org/files/issues/securepages-no_active_batch_error-1627326-26.patch",
        "Domain conf variable support": "docroot/sites/all/modules/patches/securepages_domain_conf_variable_support.patch",
        "Possibility to change HTTP response code of the URL redirect": "https://www.drupal.org/files/issues/securepages-select-http-code_2.patch"
      },
      "drupal/media_feeds": {
        "Only variables should be passed by reference in MediaFeedsLibraryProvider constructor fix": "docroot/sites/all/modules/patches/media_feeds.only-var-should-pass-by-reference.patch",
        "When column is blank, delete the media from mapping field": "docroot/sites/all/modules/patches/media_feeds.file-delete-on-empty-cases.patch"
      },
      "drupal/webform_multiple_file": {
        "Fix dependency": "docroot/sites/all/modules/patches/webform_multiple_file.fixdependency.patch"
      },
      "drupal/rabbit_hole": {
        "Expand functionality for mandatory redirect": "docroot/sites/all/modules/patches/rabbit_hole_mandatory_redirect.patch"
      },
      "drupal/search_api": {
        "Remove fields prefix": "docroot/sites/all/modules/patches/search_api_change_fields_name.patch"
      },
      "drupal/advpoll": {
        "Update poll results if poll options are modified": "docroot/sites/all/modules/patches/advpoll_modify_result_if_options_modified.patch"
      },
      "drupal/publish_button": {
        "Display link instead button  on the View Page": "docroot/sites/all/modules/patches/publish-button-7-1-1-remove-nake-link.patch"
      },
      "drupal/views": {
        "Warning: Invalid argument supplied for foreach() in views_join->build_join()": "https://www.drupal.org/files/issues/views-invalid_argument-2866370-14.patch"
      },
      "drupal/solr_devel": {
        "Ability to proxy any request to Drupal": "https://www.drupal.org/files/issues/ability_to_proxy_any-2840059-4.patch"
      },
      "drupal/commerce_feeds": {
        "Incompatibility with feeds FeedsProcessor::entityValidate": "https://www.drupal.org/files/issues/2019-05-15/FeedsProcessor-entityValidate-3050702-1.patch"
      },
      "drupal/views_selective_filters": {
        "Address Field country not working with human readable names": "docroot/sites/all/modules/patches/views_selective_filters-select_list_key_fix-2504309-6-v2.patch"
      },
      "drupal/search_api_et": {
        "Index language UND fix": "docroot/sites/all/modules/patches/search_api_et.opt-index.und-lang_v2.patch"
      }
    }
